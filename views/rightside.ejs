<div class="col s7">
  <h5>Posts</h5>
  <ul class="collection">
    <% for(var i=0; i<posts.length; i++) { %>
      <li class="collection-item avatar">
        <i class="material-icons circle red">comment</i>
        <span class="title">
          <a href="/users/<%=posts[i]._id %>" style="float:right" class="">
        <i class="material-icons">open_in_new</i></a>
        <a href="/users/delete/<%=posts[i]._id %>"  style="float:right" class="">
        <i class="material-icons">delete</i></a>
       <b>
            <%=posts[i].topic %>
          </b>
        </span>
       
          <p><%=posts[i].entry %>
            <br>
            <%=posts[i].date %>
        <br>
        
      </li>
      <% } %>
  </ul>
</div>